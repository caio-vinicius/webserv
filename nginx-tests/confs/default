server {
	listen 80;
	listen [::]:80;

	root /var/www/html;

	index index.html index.htm index.nginx-debian.html;

	server_name _;

	location / {
		try_files $uri $uri/ =404;
	}
}

server {
	listen 81;
	listen [::]:81;

	root /var/www/html; server_name banana;
	index sla.html;

	location / {
		try_files $uri $uri/ =404;
	}
}
